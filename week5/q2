
#include <bits/stdc++.h>

using namespace std;


void findPairs(int arr1[], int , int n, int x)
{
	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
			if (arr1[i] + arr2[j] == x)
				cout << arr1[i] << " "
					<< arr2[j] << endl;
}


int main()
{
	int arr1[] = { 1, 2, 3, 7, 5, 4 };
	int n = sizeof(arr1) / sizeof(int);
	int x = 8;
	findPairs(arr1, arr2, n, m, x);
	return 0;
}
